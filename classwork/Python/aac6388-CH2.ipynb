    "def decimal_to_binary(a):\n",
    "    if a == 0:\n",
    "        return \"0\"\n",
    "    \n",
    "    binary_string = \"\"\n",
    "    while a > 0:\n",
    "        remainder = a % 2\n",
    "        binary_string = str(remainder) + binary_string\n",
    "        a = a // 2\n",
    "        \n",
    "    return binary_string\n",
    "\n",
    "print(decimal_to_binary(2025))"
   ]
